  <div id="beerdetailsdiv">

<p>First Brewed: {{ this.first_brewed }}</p>
<p>abv: {{ this.abv }}</p>
<p>ibu: {{ this.ibu }}</p>
<p>Target fg: {{ this.target_fg }}</p>
<p>Target og: {{ this.target_og }}</p>
<p>ebc: {{ this.ebc }}</p>
<p>srm: {{ this.srm }}</p>
<p>ph: {{ this.ph }}</p>
<p>Attenuation Level: {{ this.attenuation_level }}</p>
<p>Volume:</p>
 <ul>
     {{#each this.volume}}
    <li> {{@key}}: {{this}}</li>
    {{/each}}
  </ul>
  <p>Boil Volume:</p>
   <ul>
     {{#each this.boil_volume}}
    <li> {{@key}}: {{this}}</li>
    {{/each}}
  </ul>
<p>Method:</p>
<ul>
    <li>Mash Temp:</li>
        <ul>{{#each this.method.mash_temp}}
    <li>{{#each this}}</li>
    {{/each}}</ul>
    <li>Fermentation</li>
    <li>Twist</li>
</ul>


  </div>

